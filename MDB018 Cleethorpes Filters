#total mg N over the course of a year
"N.annual.mg" <-308442800000000
print(N.annual.mg)

#total mg (N) daily if equal
N.daily.mg <- (N.annual.mg/365)
#daily litre effluent
L.daily.effluent <- 1000000 
average.N.mgperlitre <- (N.daily.mg/L.daily.effluent)
print(average.N.mgperlitre)

